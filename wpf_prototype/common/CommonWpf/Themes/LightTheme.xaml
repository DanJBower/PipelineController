<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Decent colour picker https://hslpicker.com/ -->
    <SolidColorBrush x:Key="AActiveBrush" Color="#3ccc4d" />
    <SolidColorBrush x:Key="BActiveBrush" Color="#c24242" />
    <SolidColorBrush x:Key="XActiveBrush" Color="#40bdc2" />
    <SolidColorBrush x:Key="YActiveBrush" Color="#dbca33" />
    <SolidColorBrush x:Key="ButtonActiveBrush" Color="#b24e01" />
    <SolidColorBrush x:Key="ButtonNotActiveBrush" Color="#282828" />
</ResourceDictionary>
